{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "@no_idea/eslint-config-ts"
  ],
  "rules": {
    "no-console": ["off"],
    "sort-imports": ["off"],
    "dot-location": ["error","property"],
    "max-len": ["error",100]
  }
}
